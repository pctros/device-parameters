{
    "description": "ZEN15 VER 2.0 Power switch",
    "parameters": [
        {
            "parameterNumber": 20,
            "label": {
                "en": "Overload Protection Delay",
                "pl": ""
            },
            "description": {
                "en": "Decide when overload protection kicks in. If power draw exceeds 16.5 A for x amount of seconds, the plug will automatically turn off.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 2,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "range",
                        "min": 2,
                        "max": 5,
                        "step": 1,
                        "scale": 1.0
                    }
                ]
            }
        },
        {
            "parameterNumber": 21,
            "label": {
                "en": "On/Off Status After Power Failure",
                "pl": ""
            },
            "description": {
                "en": "Choose the recovery state for your Power Switch if power outage occurs.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "remembers and restores status",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "always on once restored",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "always off once restored",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 24,
            "label": {
                "en": "On/Off Status Reports",
                "pl": ""
            },
            "description": {
                "en": "Choose when you want the device to send on/off reports to the hub and associated devices.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 3,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Function disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Send a Basic Report whenever the load state changes",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "Send a Basic Report only if the load state change was not triggered by a Z-Wave command",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 3,
                        "description": {
                            "en": "Send a Switch Binary Report whenever the load state changes",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 4,
                        "description": {
                            "en": "Send a Switch Binary Report only if the load state change was not triggered by a Z-Wave command",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 27,
            "label": {
                "en": "LED Indicator Behavior",
                "pl": ""
            },
            "description": {
                "en": "Decide how the LED indicator displays power consumption on the Power Switch and when itâ€™s on or off.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "LED always on with power use display",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "LED on when plug on (with power use display), LED off when plug off",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 2,
                        "description": {
                            "en": "LED on with power use for 5 seconds after status change (off otherwise)",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 3,
                        "description": {
                            "en": "LED always off",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 30,
            "label": {
                "en": "Disable Manual Control",
                "pl": ""
            },
            "description": {
                "en": "Disable or enable manual control (turning the Power Switch on and off using the physical button).",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "manual control disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "manual control enabled",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 31,
            "label": {
                "en": "Disable Z-Wave ON Commands",
                "pl": ""
            },
            "description": {
                "en": "Ignore Z-Wave ON commands sent from the hub to the Power Switch. Turn it ON only from the button. You can still turn the plug OFF via Z-Wave.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Z-Wave on commands disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Z-Wave on commands enabled",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 32,
            "label": {
                "en": "Disable Z-Wave OFF Commands",
                "pl": ""
            },
            "description": {
                "en": "Ignore Z-Wave OFF commands sent from the hub to the Power Switch. Turn it OFF only from the button. You can still turn the plug ON via Z-Wave.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 1,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "Z-Wave off commands disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "value",
                        "value": 1,
                        "description": {
                            "en": "Z-Wave off commands enabled",
                            "pl": ""
                        }
                    }
                ]
            }
        },
        {
            "parameterNumber": 33,
            "label": {
                "en": "Auto-on Timer",
                "pl": ""
            },
            "description": {
                "en": "Auto-on timer will automatically turn the Power Switch on after x minutes once it has been turned off.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "timer disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 99,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "min"
                    }
                ]
            }
        },
        {
            "parameterNumber": 34,
            "label": {
                "en": "Auto-off Timer",
                "pl": ""
            },
            "description": {
                "en": "Auto-off timer will automatically turn the Power Switch off after x minutes once it has been turned on.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "type": "value",
                        "value": 0,
                        "description": {
                            "en": "timer disabled",
                            "pl": ""
                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 99,
                        "step": 1,
                        "scale": 1.0,
                        "unit": "min"
                    }
                ]
            }
        },
        {
            "parameterNumber": 35,
            "label": {
                "en": "Turn On Delay After Power Outage",
                "pl": ""
            },
            "description": {
                "en": "Disable or set the delay for the automatic turn on after the plug was turned off due to power outage.",
                "pl": ""
            },
            "size": 1,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "description": {
                            "en": "no delay",
                            "pl": ""
                        },
                        "type": "value",
                        "value": 0
                    },
                    {
                        "max": 65535,
                        "min": 0,
                        "scale": 1,
                        "step": 1,
                        "type": "range",
                        "unit": "seconds"
                    }
                ]
            }
        },
        {
            "parameterNumber": 36,
            "label": {
                "en": "Auto Turn On After Overload",
                "pl": ""
            },
            "description": {
                "en": "Disable or set the delay for the automatic turn on after the plug was turned off due to overload protection.",
                "pl": ""
            },
            "size": 2,
            "defaultValue": 0,
            "format": {
                "type": "enumerated",
                "values": [
                    {
                        "description": {
                            "en": "auto-turn on disabled",
                            "pl": ""
                        },
                        "type": "value",
                        "value": 0
                    },
                    {
                        "max": 65535,
                        "min": 10,
                        "scale": 1,
                        "step": 1,
                        "type": "range",
                        "unit": "seconds"
                    }
                ]
            }
        },
        {
            "parameterNumber": 151,
            "label": {
                "en": "Watt Report Threshold",
