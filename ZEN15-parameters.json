                        }
                    },
                    {
                        "type": "range",
                        "min": 1,
                        "max": 65535,
                        "step": 1,
                        "scale": 1.0
                    }
                ]
            }
        },
        {
            "parameterNumber": 152,
            "label": {
                "en": "Watt Report % Threshold",
